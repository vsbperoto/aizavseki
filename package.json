{
  "name": "aizavseki-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev:web",
    "dev:web": "npm run dev --workspace=web",
    "dev:agent": "npm run dev --workspace=agent",
    "build": "npm run build:web",
    "build:web": "npm run build --workspace=web",
    "build:agent": "npm run build --workspace=agent",
    "lint": "npm run lint:web",
    "lint:web": "npm run lint --workspace=web",
    "lint:agent": "npm run lint --workspace=agent"
  }
}
